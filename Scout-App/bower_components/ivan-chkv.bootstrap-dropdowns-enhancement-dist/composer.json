{
  "name": "ivan-chkv/bootstrap-dropdowns-enhancement-dist",
  "description": "Ready to use Bootstrap Dropdowns Enhancement",
  "keywords": ["bootstrap", "dropdowns", "enhancement"],
  "homepage": "http://behigh.github.io/bootstrap_dropdowns_enhancement/",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "behigh"
    }
  ],
  "scripts": {
    "post-update-cmd": [
      "rm -frv js css",
      "mkdir -pv js",
      "cp -v vendor/behigh/bootstrap_dropdowns_enhancement/dist/js/* js",
      "mkdir -pv css",
      "cp -v vendor/behigh/bootstrap_dropdowns_enhancement/dist/css/* css"
    ]
  },
  "require-dev": {
    "behigh/bootstrap_dropdowns_enhancement": "v3.1.1"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "behigh/bootstrap_dropdowns_enhancement",
        "version": "v3.1.1",
        "dist": {
          "type": "zip",
          "url": "https://github.com/ivan-chkv/bootstrap_dropdowns_enhancement/archive/v3.1.1.zip"
        },
        "source": {
          "type": "git",
          "url": "https://github.com/ivan-chkv/bootstrap_dropdowns_enhancement.git",
          "reference": "v3.1.1"
        }
      }
    }
  ]
}
